{
	//Configuration
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "request-logger",
	"main": "src/index.ts",

	//Settings
	"compatibility_date": "2025-06-17",
	"placement": { "mode": "smart" },
	"observability": {
		"enabled": true
	},

	//Trigger
	"workers_dev": false,
	"preview_urls": false,
	
	//Bindings
	"analytics_engine_datasets": [
		{
			"binding": "AED",
			"dataset": "domain-calls"
		}
	]
}
